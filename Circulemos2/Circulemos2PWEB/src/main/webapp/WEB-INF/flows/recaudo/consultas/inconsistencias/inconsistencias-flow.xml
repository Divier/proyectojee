<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/webflow
        http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="consultarInconsistenciasRecaudoHolderFL"
		class="co.com.datatools.c2.managed_bean.recaudo.consultas.inconsistencias.ConsultarInconsistenciasRecaudoHolderFL" />

	<view-state id="ingresar"
		view="/WEB-INF/jsp/recaudo/consultas/inconsistencias/consultarInconsistenciasRecaudo.xhtml">
		<on-entry>
			<evaluate
				expression="externalContext.sessionMap.circulemosUtilMB.instanciarMB('consultarInconsistenciasRecaudoMB')" />
		</on-entry>
		<transition on="_op-CONSULTAR" to="_op-CONSULTAR" />
	</view-state>

	<action-state id="_op-CONSULTAR">
		<evaluate
			expression="externalContext.sessionMap.consultarInconsistenciasRecaudoMB.consultarInconsistenciasRecuado()" />
		<transition on="success" to="ingresar" />
	</action-state>

</flow>